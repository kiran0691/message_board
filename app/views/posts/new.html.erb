<div id="new-post">
  <h4>New Post</h4>
    <%= form_for [@user,@post], :html => {:class => "form-horizontal center"} do |f|%>
      <div class="form-group new-title" id = "new-title">
        <%= f.label :title %>
        <%= f.text_field :tile, class: "form-control" %>
      </div>
      <div class="form-group new-body">
        <%= f.label :body %>
        <%= f.text_area :body, class: "form-control", rows: "10" %>
      </div>
      <div>
      <%= f.submit "Submit", class: "btn btn-default btn-primary"%>
      <%= link_to "Back", posts_path, class: "btn btn-default btn-primary" %>
    </div>
    <%end%>
</div>
